{
    "name": "@okikio/event-emitter",
    "version": "1.0.10",
    "description": "A small Event Emitter written in typescript for performance, and ease of use.",
    "publishConfig": {
        "access": "public"
    },
    "main": "lib/api.modern.js",
    "umd:main": "lib/api.umd.js",
    "browser": "lib/api.umd.js",
    "unpkg": "lib/api.umd.js",
    "module": "lib/api.es.js",
    "source": "src/api.ts",
    "types": "lib/api.d.ts",
    "files": [
        "lib",
        "src"
    ],
    "scripts": {
        "build": "microbundle build --name @okikio/event-emitter --output lib/api.js --external natives",
        "watch": "microbundle watch --name @okikio/event-emitter --output lib/api.js --external natives",
        "release": "npm run build && npm publish --tag latest",
        "patch": "npm version patch && npm run release",
        "minor": "npm version minor && npm run release",
        "major": "npm version major && npm run release"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/okikio/event-emitter.git"
    },
    "keywords": [
        "event",
        "emitter",
        "small",
        "js",
        "typescript",
        "api"
    ],
    "author": {
        "name": "Okiki Ojo",
        "email": "hey@okikio.dev",
        "url": "https://okikio.dev"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/okikio/event-emitter/issues"
    },
    "homepage": "https://github.com/okikio/event-emitter#readme",
    "devDependencies": {
        "eslint": "^7.3.0",
        "microbundle": "^0.12.2",
        "typescript": "^3.9.5"
    },
    "dependencies": {
        "managerjs": "^1.1.0"
    }
}
