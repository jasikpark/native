{% extends "layouts/layout.njk" %}

{% block content %}
    <main data-wrapper id="404-page" role="main" id="sb-wrapper">
        <div id="page-home-page"
            class="page-content page-content-ajax"
            data-node-name="home-page"
            data-is-home="1"
            data-node-type="HomePage"
            data-meta-title="Home page">
            <div class="container">
                <div class="row">
                    <div class="col">
                        <div class="jumbotron jumbotron-fluid my-3 bg-white rounded box-shadow">
                            <div class="container">
                                <h1 class="display-4">404 Page</h1>
                                <p class="lead">Dynamic blocks example.</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col">
                        <div class="page-block usersblock"
                            id="users-block-1"
                            data-node-type="UsersBlock">
                            <div class="usersblock__intro d-flex align-items-center bg-white p-3 my-3 text-white-50 rounded box-shadow">
                                <div class="avatar-cont"></div>
                                <div class="lh-100">
                                    <h6 class="mb-0 lh-100">Starting Blocks</h6>
                                    <small>By Rezo Zero</small>
                                </div>
                            </div>
                            <div class="my-3 p-3 bg-white rounded box-shadow">
                                <h6 class="border-bottom border-gray pb-2 mb-0">Contributors</h6>
                                <div class="row usersblock__contributors-list"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <footer class="footer">
            <div class="container">
                <a href="https://github.com/rezozero/starting-blocks" class="text-muted" target="_blank">Starting blocks</a>
            </div>
        </footer>
    </main>
{% endblock %}