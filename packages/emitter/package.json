{
    "name": "@okikio/emitter",
    "version": "2.1.2",
    "type": "module",
    "sideEffects": false,
    "description": "A small Event Emitter written in typescript with performance and ease of use in mind.",
    "publishConfig": {
        "access": "public",
        "main": "lib/api.cjs.js",
        "types": "@types/api.d.ts",
        "browser": "lib/api.es.js",
        "module": "lib/api.es.js",
        "exports": {
            ".": {
                "require": "./lib/api.cjs.js",
                "import": "./lib/api.es.js",
                "default": "./lib/api.es.js"
            },
            "./lib/*": "./lib/*.js"
        }
    },
    "main": "src/api.ts",
    "directories": {
        "lib": "./lib"
    },
    "files": [
        "lib",
        "src",
        "@types"
    ],
    "scripts": {
        "build": "node ./build",
        "watch": "node ./build --watch",
        "test": "jest --maxWorkers=1",
        "test-dev": "jest --maxWorkers=1 --watch --verbose false",
        "dts": "tsc --project dts.tsconfig.json",
        "pre-release": "npm run build && npm test && npm run dts",
        "changelog": "standard-version --releaseCommitMessageFormat --tag-prefix \"@okikio/emitter@\" \"chore(release): @okikio/emitter@{{currentTag}}\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/okikio/native.git"
    },
    "keywords": [
        "event",
        "emitter",
        "small",
        "light-weight",
        "ts",
        "typescript",
        "api"
    ],
    "author": {
        "name": "Okiki Ojo",
        "email": "hey@okikio.dev",
        "url": "https://okikio.dev"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/okikio/native/issues"
    },
    "homepage": "https://github.com/okikio/native/tree/master/packages/emitter#readme",
    "dependencies": {
        "@okikio/manager": "workspace:*"
    },
    "devDependencies": {
        "@types/jest": "^26.0.22",
        "esbuild": "^0.11.13",
        "esbuild-jest": "^0.5.0",
        "jest": "^26.6.3",
        "jest-chain": "^1.1.5",
        "pnpm": "^6.0.2",
        "standard-version": "^9.2.0",
        "typescript": "^4.2.4",
        "ultra-runner": "^3.10.5"
    },
    "gitHead": "ad422076cdb57d570426f786886129d5bca2f90f"
}
