{
    "name": "@okikio/animate",
    "version": "2.0.0",
    "description": "An animation library for the modern web which utilizes the Web Animation API (WAAPI) to create butter smooth animation.",
    "publishConfig": {
        "access": "public",
        "main": "lib/api.cjs.js",
        "types": "@types/api.d.ts",
        "browser": "lib/api.es.js",
        "module": "lib/api.es.js",
        "exports": {
            ".": {
                "require": "./lib/api.cjs.js",
                "import": "./lib/api.es.js",
                "default": "./lib/api.es.js"
            },
            "./src/*": "./src/*.ts",
            "./lib/*": "./lib/*.js",
            "./ts": "./src/api.ts",
            "./es": "./lib/api.es.js",
            "./cjs": "./lib/api.cjs.js",
            "./iife": "./lib/api.js"
        }
    },
    "main": "src/api.ts",
    "source": "src/api.ts",
    "directories": {
        "lib": "./lib"
    },
    "files": [
        "lib",
        "src",
        "@types"
    ],
    "scripts": {
        "build": "node ./build",
        "watch": "node ./build --watch",
        "dts": "tsc --project dts.tsconfig.json",
        "pre-release": "npm run build && npm run dts",
        "changelog": "standard-version --releaseCommitMessageFormat --tag-prefix \"@okikio/animate@\" \"chore(release): @okikio/animate@{{currentTag}}\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/okikio/native.git"
    },
    "keywords": [
        "ts",
        "modern",
        "animation",
        "library",
        "web",
        "css",
        "smooth"
    ],
    "author": {
        "name": "Okiki Ojo",
        "email": "hey@okikio.dev",
        "url": "https://okikio.dev"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/okikio/native/issues"
    },
    "homepage": "https://github.com/okikio/native/packages/animate#readme",
    "dependencies": {
        "@okikio/emitter": "workspace:*",
        "@okikio/manager": "workspace:*",
        "esbuild": "^0.8.31",
        "gzip-size": "^6.0.0",
        "pretty-bytes": "^5.5.0",
        "standard-version": "^9.1.0",
        "typescript": "^4.1.3"
    },
    "devDependencies": {
        "chokidar": "^3.5.0"
    },
    "gitHead": "ad422076cdb57d570426f786886129d5bca2f90f"
}
