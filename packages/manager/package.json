{
    "name": "@okikio/manager",
    "version": "1.1.2",
    "description": "A superset of the Map class, it gives the Map class superpowers.",
    "publishConfig": {
        "access": "public",
        "main": "lib/api.cjs.js",
        "types": "@types/api.d.ts"
    },
    "main": "src/api.ts",
    "browser": "lib/api.es.js",
    "module": "lib/api.es.js",
    "exports": {
        ".": {
            "require": "./lib/api.cjs.js",
            "import": "./lib/api.es.js",
            "default": "./lib/api.es.js"
        },
        "./src/*": "./src/*.ts",
        "./lib/*": "./lib/*.js",
        "./ts": {
            "require": "./src/api.ts",
            "import": "./src/api.ts",
            "default": "./src/api.ts"
        },
        "./es": "./lib/api.es.js",
        "./cjs": "./lib/api.cjs.js",
        "./iife": "./lib/api.js"
    },
    "source": "src/api.ts",
    "directories": {
        "lib": "./lib"
    },
    "files": [
        "lib",
        "src",
        "@types"
    ],
    "scripts": {
        "build": "node ./build",
        "watch": "node ./build --watch",
        "test": "jest --maxWorkers=1",
        "test:watch": "jest --maxWorkers=1 --watch --verbose false",
        "dts": "tsc --project dts.tsconfig.json",
        "pre-release": "npm run build && npm test && npm run dts",
        "changelog": "standard-version --releaseCommitMessageFormat --tag-prefix \"@okikio/manager@\" \"chore(release): @okikio/manager@{{currentTag}}\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/okikio/native.git"
    },
    "keywords": [
        "manager",
        "ts",
        "map",
        "library",
        "web",
        "api"
    ],
    "author": {
        "name": "Okiki Ojo",
        "email": "hey@okikio.dev",
        "url": "https://okikio.dev"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/okikio/native/issues"
    },
    "homepage": "https://github.com/okikio/native/packages/manager#readme",
    "dependencies": {
        "esbuild": "^0.8.27",
        "gzip-size": "^6.0.0",
        "pretty-bytes": "^5.5.0",
        "standard-version": "^9.1.0",
        "typescript": "^4.1.3"
    },
    "devDependencies": {
        "@skypack/package-check": "^0.2.2",
        "@types/jest": "^26.0.19",
        "chokidar": "^3.4.3",
        "esbuild-jest": "^0.3.0",
        "eslint": "^7.16.0",
        "jest": "^26.6.3",
        "jest-chain": "^1.1.5",
        "node-fetch": "^2.6.1",
        "ultra-runner": "^3.8.0"
    },
    "gitHead": "ad422076cdb57d570426f786886129d5bca2f90f"
}
